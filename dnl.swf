											} else {
						_this.trace("WARNING:\n    The medal you unlocked does not appear to exist (medal id# "+medal_id+").\n    If you recently added this medal, you may need to reload this page.");
					}

					if ($('#medal_'+medal_id)) {
						$('#medal_click_'+medal_id).hide();
