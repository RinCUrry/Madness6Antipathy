		var ApiController = (function ($) {
			return function() {

				var controller = this;

				this.getURL = function(url) {
					$("#api_features").load(url, function() {
						controller.initialize();
					});
				};

				var loadFromClick = function(e) {
					if (this.href && this.href.length) {
						controller.getURL(this.href);
					}

					return false;
				};

				this.initialize = function() {
					// these are the buttons in the header of the api items
					// pod, which can be found in portal entries like
					// https://www.newgrounds.com/portal/view/610807
					$("#api_buttons a").click(loadFromClick);

					$("#api_feature a").click(loadFromClick);

					$("#api_user_filter a").click(loadFromClick);

					$("#api_pagination a").click(loadFromClick);

					$("#usergenerated_icons a[name^='loadfile_']")
						.click(function(e) {
							e.preventDefault();
							var save_id = $(this).attr('id');
							if (save_id) {
								var save_info = save_id.split("_");
								var group_id = parseInt(save_info[1]);
								var file_id = parseInt(save_info[3]);
								var href = $(this).attr('href');
								window.postMessage('{event:"load_save_file",href:"'+href+'",group:'+group_id+',file:'+file_id+'}','*');
							}
							return false;
						});

					$('#usergenerated_group') // deprecated, only used by old flash shit
						.change(function(e) {
							var baseURL = document.getElementById('submission_url').value;
							baseURL = baseURL.replace("/portal/view/","/ngio/api_items/");
							controller.getURL(baseURL+'/usergenerated/browse/'+this.value);
						});

					$('#scoreboard_select')
						.change(function(e) {
							controller.getURL($(this).val());
						});

					$('#scoreboard-user-lookup')
						.submit(function(e) {
							e.preventDefault();
							controller.getURL($(this).attr('action')+"?username="+$("input[name='username'").first().val());
						});

					$('#medal_group, #medals_sort, #savegroups_sort')
						.off()
						.change(function(e) {
							controller.getURL(this.value);
						});
				};

				// used to prevent flooding
				this.lastRefresh = null,

				// called if a game posts a valid ngioComponent message
				this.triggerUpdate = function(data) {

					if (this.lastRefresh !== null) {
						let now = new Date();
						let diff = (now - this.lastRefresh)/1000;
						if (diff < 3) return;
					}

					this.lastRefresh = new Date();

					let $features = $("#api_features");
					let $active = $("a[id^='api_feature'].active", $("#api_buttons",$features));
					let active = $active.attr('id');
					setTimeout(function() {

						switch(data.ngioComponent) {

							case "Medal.unlock":
								if (active === "api_feature_medals") {
									controller.getURL($active.attr('data-href'));
								}
								break;

							case "ScoreBoard.postScore":
								if (active === "api_feature_scores") {
									let $select = $('#scoreboard_select', $features);
									let score_id = parseInt($select.val().split("/scores/").pop().split("/").shift());
									if (data.id == score_id) $select.trigger('change');
								}
								break;
						}

					},500);
				}
			}
		})(jQuery);

